<h2>New Review for <%= @restaurant.name %></h2>

<%= simple_form_for [@restaurant, @review] do |f| %>
  <%= f.input :content, label: 'Tell us about this restaurant...' %>
  <%= f.input :rating, label: 'Your rating', collection: 0..5 %>
  <%= f.submit "Submit Review", class: "btn btn-primary"%>
<% end %>
